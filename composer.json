  "authors": [
    {
      "name": "sukhrobnuraliev",
      "email": "sukhrobnuralievv@gmail.com"
    }
  ],
  "require": {
    "filp/whoops": "^2.14",
    "vlucas/phpdotenv": "^5.5",
    "ext-pdo": "*",
    "symfony/console": "^6.2",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/panther": "^2.0",
    "dbrekelmans/bdi": "^1.0"
  }
}
